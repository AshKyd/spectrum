{
	"manifest_version": 2,

	"name": "Color Blind",
	"description": "Test the page",
	"version": "0.1",
	
	"homepage_url": "http://healthignition.com",

	"browser_action": {
		"default_title": "Color Blind",
		"default_popup": "popup.html"
	},
	
	"web_accessible_resources": [
		"filters.svg"
	],
	
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": [],
			"js": ["filters.js"]
		}
	],

	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}